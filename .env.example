# Creative Production Pipeline Configuration
# Copy this to .env and fill in your values

# Required: Get from https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=your_token_here

# Optional: Default settings
DEFAULT_QUALITY=standard  # draft, standard, premium
DEFAULT_IMAGE_MODEL=flux_schnell  # sdxl, flux_schnell, flux_dev, playground
INCLUDE_VIDEO=false  # Set to true for video generation

# Optional: Output settings
OUTPUT_DIR=./creative_outputs
SAVE_METADATA=true

# Optional: For Slack/Discord notifications
WEBHOOK_URL=
NOTIFY_ON_COMPLETE=false